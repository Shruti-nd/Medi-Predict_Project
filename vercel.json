{
  "version": 2,
  "name": "medi-predict",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20",
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/config", "dest": "/api/config.js" },
    { "src": "/api/predict", "dest": "/api/predict.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
