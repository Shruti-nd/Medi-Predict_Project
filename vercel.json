{
  "version": 2,
  "name": "medi-predict",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20.x",
      "memory": 1024
    }
  },
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/config", "dest": "/api/config.js" },
    { "src": "/api/predict", "dest": "/api/predict.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
